#!/usr/bin/env minivx

const max = 999
const min = 100

var solution = 0

var a = max

while a >= min do
{
	if a^2 < solution then
	{
		break
	}
	
	var b = a
	
	while b >= min do
	{
		const x = a * b
		
		if x <= solution then
		{
			break
		}
		
		const s = str x
		
		if s == -s then
		{
			solution = x
			break
		}
		
		--b
	}
	
	--a
}

print solution
