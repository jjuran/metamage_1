#!/usr/bin/env vx

let ESC = "\x1b"
let CSI = ESC "["

let square = "\u2588" * 2

def ramp (row)
{
	return str (row map { CSI "38;5;", v, "m" square })
}

let normals = 0 .. 7
let brights = 8 .. 15

let colors = 0 -> 6 map { let x = 36 * v + 16; x -> x + 36 }

let grays = 232 .. 255

print ramp normals

print ramp brights

for row in colors do
{
	print ramp row
}

print ramp grays

OUT <== CSI "0m"
